public class EnumTest {
	public enum Color {
		RED, GREEN, BLUE
	}

	public enum Count {
		ONE, TWO, THREE
	}
	
	public enum Season{
		ë´„("3ì›”ë¶€í„° 5ì›”ê¹Œì§€", 13),
		ì—¬ë¦„("6ì›”ë¶€í„° 8ì›”ê¹Œì§€", 25),
		ê°€ì„("3ì›”ë¶€í„° 5ì›”ê¹Œì§€", 15),
		ê²¨ìš¸("12ì›”ë¶€í„° 2ì›”ê¹Œì§€", 1);
		
		//ê°’ë“¤ì´ ì €ì¥ë  ë³€ìˆ˜ ì„ ì–¸ 
		private String span;
		private int temp;
		
		//ìƒì„±ì 
		Season(String span, int temp) { //private Season(String span, int temp){ ì™€ ê°™ë‹¤. 
			this.span = span;
			this.temp = temp;
		}
		//getter ë©”ì„œë“œ
		public String getSpan() {
			return span;
		}
		public int getTemp() {
			return temp;
		}
	}
	public static void main(String[] args) {
//		System.out.println("RED" + ConstTest.RED);
//		System.out.println("THREE" + ConstTest.THREE);
//		
//		if (ConstTest.RED == ConstTest.TWO) {
//			System.out.println("....");
//		} else {
//			System.out.println("####");
//		}

		Color mycol = Color.valueOf("GREEN"); // Color.GREEN ê³¼ ê°™ë‹¤.
		Count mycnt = Count.ONE; // Count.valueOf("ONE") ê³¼ ê°™ë‹¤.

		System.out.println("mycol: " + mycol);
		System.out.println("mycnt: " + mycnt);

		System.out.println("mycol ordinal : " + mycol.ordinal());
		System.out.println("mycnt ordinal : " + mycnt.ordinal());

		/*
		 * //ì„œë¡œ ë‹¤ë¥¸ ì¢…ë¥˜ì˜ ì—´ê±°í˜•ë¼ë¦¬ì˜ ë¹„êµëŠ” ë¶ˆê°€í•˜ë‹¤. if (mycol == mycnt) {
		 * System.out.println("ê°™ë‹¤..."); }
		 */

		if (mycol == Color.BLUE) {
			System.out.println("ê°™ë‹¤...");
		} else
			System.out.println("ê°™ë‹¤...");
		
		//ì—´ê±°í˜•ì€ switchë¬¸ìœ¼ë¡œë„ ë¹„êµí•  ìˆ˜ ìˆë‹¤. 
		//switchë¬¸ì˜ caseì— ì§€ì •í•˜ëŠ” ìƒìˆ˜ê°’ì€ 'ì—´ê±°í˜• ì´ë¦„'ì„ ìƒëµí•œ 'ìƒìˆ˜ì´ë¦„'ë§Œ ì§€ì •í•´ì•¼ í•œë‹¤. 
		switch (mycnt) {
		//case Count.ONE :System.out.println("ONE ì…ë‹ˆë‹¤."); 
		case ONE: System.out.println("ONE ì…ë‹ˆë‹¤."); break;
		case TWO: System.out.println("TWO ì…ë‹ˆë‹¤."); break;
		case THREE: System.out.println("THREE ì…ë‹ˆë‹¤."); break;
		}
		System.out.println("==============================");
		Season ss = Season.valueOf("ë´„");
		System.out.println("name: " + ss.name());
		System.out.println("ordinal: " + ss.ordinal());
		System.out.println("span: " + ss.getSpan());
		System.out.println("temp: " + ss.getTemp());
		
		for (Season time : Season.values()) {
			System.out.println(time + "==" + time.name() 
			+ "-->" + time.getSpan() + ", í‰ê· ê¸°ì˜¨: " + time.getTemp());
		}
	}
}

